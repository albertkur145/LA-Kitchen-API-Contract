NB : 
- untuk mengambil semua data produk berdasarkan kategori besar.


Header
- Content-Type   : application/json


url      : http://localhost:{port}/api/product/category
type     : GET
params   : {
    "categoryId": 2,
}


RESPONSE
--------
{
    "code": 200,
    "status": "OK",
    "data": {
        "products": [
            {
                "id": 1,
                "name": "Kue Talan Pandan",
                "price": 60000,
                "photo_link": "www.google.co.id",
                "rating": 4.73,
                "evaluators": 127,
            },
            {
                "id": 2,
                "name": "Kue Lapis",
                "price": 95000,
                "photo_link": "www.google.co.id",
                "rating": 4.25,
                "evaluators": 96,
            },
        ],
    },
    "paging": null,
    "errors": null,
}